{
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.3.0",
    "eslint": "^8.23.0",
    "ncc": "^0.3.6",
    "typescript": "^4.8.2"
  },
  "scripts": {
    "compile": "npx tsc",
    "copyDevConfig": "copyfiles development_config.json ./build",
    "prestart": "npm run compile && npm run copyDevConfig",
    "start": "node ./build/bot.js",
    "build:azure": "npm run prestart && ncc build ./build/bot.js -o dist"
  }
}
